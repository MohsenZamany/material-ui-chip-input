{
  "name": "material-ui-chip-input",
  "version": "1.1.1",
  "typings": "typings/index.d.ts",
  "description": "A chip input field using Material-UI.",
  "main": "lib/ChipInput.js",
  "files": [
    "lib/**/*.js",
    "typings/*.d.ts",
    "README.md",
    "LICENSE",
    "demo.gif"
  ],
  "scripts": {
    "build": "babel src -d lib",
    "test": "standard && jest",
    "test:coverage": "jest --coverage && cat ./coverage/lcov.info | coveralls",
    "test:unit": "jest",
    "lint": "standard",
    "prepublish": "babel src -d lib",
    "prestorybook": "npm i react-autosuggest autosuggest-highlight",
    "storybook": "start-storybook -p 6006",
    "storybook:build": "build-storybook -c stories",
    "storybook:install": "cd stories && npm install",
    "deploy-storybook": "build-storybook -o storybook && gh-pages -d storybook && rm -r storybook"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/TeamWertarbyte/material-ui-chip-input.git"
  },
  "keywords": [
    "react",
    "material",
    "chip",
    "input",
    "tags"
  ],
  "author": "Wertarbyte <kontakt@wertarbyte.com> (https://wertarbyte.com)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/TeamWertarbyte/material-ui-chip-input/issues"
  },
  "homepage": "https://github.com/TeamWertarbyte/material-ui-chip-input#readme",
  "devDependencies": {
    "@babel/cli": "^7.16.0",
    "@babel/core": "^7.16.0",
    "@babel/plugin-proposal-class-properties": "^7.16.0",
    "@babel/preset-env": "^7.16.4",
    "@babel/preset-react": "^7.16.0",
    "@material-ui/core": "^4.12.3",
    "@storybook/addon-actions": "^6.4.7",
    "@storybook/react": "^6.4.7",
    "babel-eslint": "^10.1.0",
    "babel-loader": "^8.2.3",
    "coveralls": "^3.1.1",
    "enzyme": "^3.11.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "enzyme-to-json": "^3.6.2",
    "gh-pages": "^3.2.3",
    "jest": "^27.4.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-test-renderer": "^17.0.2",
    "standard": "^16.0.4"
  },
  "peerDependencies": {
    "@material-ui/core": "^1.0.0 || ^4.12.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
  },
  "dependencies": {
    "classnames": "^2.3.1",
    "prop-types": "^15.7.2"
  },
  "jest": {
    "setupFiles": [
      "./test/shim.js",
      "./test/jestsetup.js"
    ],
    "snapshotSerializers": [
      "enzyme-to-json/serializer"
    ],
    "coveragePathIgnorePatterns": [
      "<rootDir>/test"
    ],
    "roots": [
      "src",
      "test"
    ]
  },
  "standard": {
    "parser": "babel-eslint"
  }
}
